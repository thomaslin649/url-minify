{
  "name": "url-minify",
  "version": "2.8.3",
  "description": "Library to shorten url without any api keys",
  "type": "module",
  "packageManager": "pnpm@7.2.1",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/url-minify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/url-minify.git"
  },
  "bugs": "https://github.com/rubiin/url-minify/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && clean-publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.7.13",
    "bumpp": "^8.2.1",
    "clean-publish": "^4.0.1",
    "eslint": "^8.22.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "unbuild": "^0.8.9",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "axios": "^0.27.2"
  },
  "clean-publish": {
    "cleanDocs": true,
    "cleanComments": true
  }
}
